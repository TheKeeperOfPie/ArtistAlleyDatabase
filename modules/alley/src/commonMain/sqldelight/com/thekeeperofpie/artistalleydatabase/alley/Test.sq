getArtistNotes:
SELECT artistNotes.* FROM artistNotes
ORDER BY artistNotes.artistId, artistNotes.dataYear;

getStampRallyNotes:
SELECT stampRallyNotes.* FROM stampRallyNotes
ORDER BY stampRallyNotes.stampRallyId;
