getEntry:
SELECT * FROM stampRallyEntry WHERE id = :id;

insert:
INSERT OR REPLACE INTO stampRallyEntry
VALUES ?;

insertArtistConnection:
INSERT OR REPLACE INTO stampRallyArtistConnection
VALUES ?;

getArtistEntries:
SELECT artistEntry.* FROM stampRallyArtistConnection
INNER JOIN artistEntry ON (stampRallyArtistConnection.artistId = artistEntry.id)
WHERE stampRallyArtistConnection.stampRallyId = :stampRallyId;
